{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "astro",
  "redirects": [
    {
      "source": "/",
      "has": [{ "type": "host", "value": "portal.lovethisplace.co" }],
      "destination": "/portal",
      "permanent": false
    },
    {
      "source": "/en",
      "has": [{ "type": "host", "value": "portal.lovethisplace.co" }],
      "destination": "/portal",
      "permanent": false
    },
    {
      "source": "/en/",
      "has": [{ "type": "host", "value": "portal.lovethisplace.co" }],
      "destination": "/portal",
      "permanent": false
    },
    {
      "source": "/es",
      "has": [{ "type": "host", "value": "portal.lovethisplace.co" }],
      "destination": "/portal",
      "permanent": false
    },
    {
      "source": "/es/",
      "has": [{ "type": "host", "value": "portal.lovethisplace.co" }],
      "destination": "/portal",
      "permanent": false
    },
    {
      "source": "/en/portal",
      "has": [{ "type": "host", "value": "portal.lovethisplace.co" }],
      "destination": "/portal",
      "permanent": false
    },
    {
      "source": "/es/portal",
      "has": [{ "type": "host", "value": "portal.lovethisplace.co" }],
      "destination": "/portal",
      "permanent": false
    }
  ],
  "rewrites": [
    { "source": "/favicon.ico", "destination": "/favicon.svg" }
  ]
}
