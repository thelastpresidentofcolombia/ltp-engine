{
  "name": "ltp-engine",
  "type": "module",
  "version": "1.0.0",
  "description": "Multi-vertical static business factory engine",
  "scripts": {
    "dev": "set DEBUG= && astro dev",
    "dev:verbose": "astro dev",
    "start": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "validate": "tsx scripts/validate-operators.ts && tsx scripts/validate-vibes.ts",
    "prebuild": "npm run validate",
    "lint": "eslint src --ext .ts,.astro",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@astrojs/tailwind": "^5.0.0",
    "@astrojs/vercel": "^7.8.2",
    "astro": "^4.0.0",
    "stripe": "^14.0.0",
    "tailwindcss": "^3.4.0"
  },
  "devDependencies": {
    "@astrojs/check": "^0.3.0",
    "@types/node": "^20.0.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "prettier-plugin-astro": "^0.12.0",
    "tsx": "^4.0.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
